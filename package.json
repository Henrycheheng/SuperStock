{
  "name": "test-app",
  "version": "0.0.0",
  "scripts": {
    "dev": "npx --max_old_space_size=4096 vite",
    "build": "run-p type-check build-only",
    "serve": "npx --max_old_space_size=4096 vite",
    "preview": "pnpm run build && vite preview --port 4173",
    "test:unit": "vitest --environment jsdom",
    "test:e2e": "start-server-and-test preview http://localhost:4173/ 'cypress open --e2e'",
    "test:e2e:ci": "start-server-and-test preview http://localhost:4173/ 'cypress run --e2e'",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint:eslint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "lint:prettier": "prettier --write --loglevel warn \"src/**/*.{js,css,json,tsx,html,vue,less,md}\"",
    "lint:style": "stylelint --fix \"**/*.{css,tsx,html,vue,less}\" --cache --cache-location node_modules/.cache/stylelint",
    "lint:ls-lint": "ls-lint",
    "lint:lint-staged": "lint-staged -c .husky/lintstagedrc.js",
    "lint:pretty": "pretty-quick --staged",
    "install:husky": "is-ci || husky install",
    "postinstall": "npm run install:husky",
    "clean:cache": " rimraf node_modules/.cache/ rimraf node_modules/.vite",
    "reinstall": "rimraf pnpm-lock.yaml && rimraf node_modules && pnpm install"
  },
  "dependencies": {
    "pinia": "^2.0.17",
    "vue": "^3.2.37",
    "vue-router": "^4.1.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@ls-lint/ls-lint": "^1.11.2",
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/jsdom": "^20.0.0",
    "@types/node": "^16.11.47",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "@vitejs/plugin-vue": "^3.0.1",
    "@vitejs/plugin-vue-jsx": "^2.0.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.0.2",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.8",
    "cypress": "^10.4.0",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.3.0",
    "husky": "^8.0.1",
    "is-ci": "^3.0.1",
    "jsdom": "^20.0.0",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "rimraf": "^3.0.2",
    "start-server-and-test": "^1.14.0",
    "stylelint": "^14.11.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^28.0.0",
    "stylelint-order": "^5.0.0",
    "typescript": "~4.7.4",
    "vite": "^3.0.4",
    "vitest": "^0.21.0",
    "vue-eslint-parser": "^9.0.3",
    "vue-tsc": "^0.39.5"
  }
}
